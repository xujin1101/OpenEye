<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.eye.eyepetizer">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".base.EyeApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".welcome.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".discover.topic.TopIcActivity"/>
        <activity android:name=".my.mymessage.MyMessageActivity"/>
        <activity android:name=".discover.top.TopActivity"/>
        <activity android:name=".author.authorDetail.AuthorDetail"/>
        <activity android:name=".discover.glideview.GlideViewNextActivity"/>
        <activity android:name=".discover.panorama.PanoramaNextActivity"/>
        <activity android:name=".discover.banner.BannerFirstNextActivity"/>
        <activity android:name=".my.MyCollectActivity"/>
    </application>

</manifest>